{
  "type": "module",
  "name": "@dvanauken/sphere",
  "version": "0.1.0-alpha.1001",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prebuild": "git add . && (git commit -m \"Pre-release commit\" || exit 0)",
    "version:bump": "npm version prerelease --preid=alpha",
    "release": "npm run prebuild && npm run build && npm run version:bump && npm publish --tag latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvanauken/sphere.git"
  },
  "files": [
    "build/**/*",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^7.32.0",
    "tsx": "^4.19.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "typescript",
    "geometry",
    "geospatial",
    "spherical-geometry",
    "navigation"
  ],
  "author": "dvanauken",
  "license": "Proprietary"
}
